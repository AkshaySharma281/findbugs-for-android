<?xml version="1.0" encoding="UTF-8"?>
<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	pluginid="jp.f.dev.findbugs.detect.android"
	provider="f.dev">
	<Detector class="jp.f.dev.findbugs.detect.android.FindAndroidStream"
		speed="moderate" reports="ANDROID_UNCLOSED_CURSOR,ANDROID_UNCLOSED_RESOURCE" />
	<BugPattern abbrev="AUCCUR" type="ANDROID_UNCLOSED_CURSOR"
		category="CORRECTNESS" />
	<BugPattern abbrev="AUCRSC" type="ANDROID_UNCLOSED_RESOURCE"
		category="CORRECTNESS" />	
</FindbugsPlugin>
